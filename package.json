{
  "name": "GV_tracker",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.9",
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/http-client": "^4.4.0",
    "@sap-cloud-sdk/util": "^4.4.0",
    "@sap/cds": "^9",
    "@sap/cds-common-content": "^3.1.0",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.12.2",
    "express": "^4",
    "hdb": "^2.27.1",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/cds-types": "^0.15.0",
    "@cap-js/sqlite": "^2",
    "@sap/dev-cap-tools": "^1.31.0",
    "@types/node": "^24.0.0",
    "tsx": "^4",
    "typescript": "^5",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch": "cds-tsx watch",
    "hybrid": "cds watch --profile hybrid",
    "bind-hdi": "cds bind -2 hana-hdi:default",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-customergiftissue": "cds watch --open customergiftissue/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "[development]": {
        "db": "hana",
        "auth": "dummy"
      }
    }
  },
  "lcap": true,
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "workspaces": [
    "app/*"
  ]
}
